{
    "name": "tnware/grav-theme-tango",
    "type": "grav-theme",
    "description": "TANGO is a free and responsive theme for **Grav** forked from Grav's theme -- a port of Twenty template by HTML5 UP.",
    "keywords": ["theme","grav", "twenty"],
    "homepage": "http://tnware.net",
    "license": "MIT",
    "support": {
        "irc": "irc://irc.freenode.org/grav"
    },
    "authors": [
        {
            "name": "Tyler Woods",
            "email": "tw@tnware.net",
            "homepage": "http://tnware.net"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "composer/installers": "~1.0"
    },
    "extra": {
        "installer-name": "tango",
        "installer-paths": {
            "user/themes/{$name}/": ["tnware/grav-theme-tango"]
        }
    }
}
